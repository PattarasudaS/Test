@if (ViewBag.message == "Received")
{
    <h3>รับเรื่องแจ้งซ่อม</h3>

    <div class="widget widget-nopad">
        <div class="widget-header">
            <span class="fa fa-search"></span>
            <h3>ค้นหาข้อมูล</h3>
        </div>
        <!-- /widget-header -->
        <div class="widget-content" id="additional-customer-detail">
            <div class="row">
                <form method="post">
                    <div class="row">
                        <div class="col-xs-12 col-sm-6 col-lg-4">
                            <label class="control-label" for="firstname">ชื่อ</label>
                            <div class="controls">
                                <input type="text" id="firstname" value=UserDetail.CodeName>
                            </div>
                        </div>

                        <div class="col-xs-12 col-sm-6 col-lg-4">
                            <label class="control-label" for="lastname">นามสกุล</label>
                            <div class="controls">
                                <input type="text" id="lastname" value="5555">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12 col-sm-6 col-lg-4">
                            <label class="control-label">โครงการ</label>
                            <div class="controls">
                                <select id="Project">
                                    <option>2078 - เดอะ เบส โคราช [Active]</option>
                                    <option>2066 - ดี คอนโด กาญจนวนิช [Active]</option>
                                    <option>2077 - ดี คอนโด ฮาย [Active]</option>
                                    <option>2121 - เดอะ เทอร์ทีไนน์ [Active]</option>
                                </select>
                            </div>
                        </div>

                        <div class="col-xs-12 col-sm-6 col-lg-4">
                            <label class="control-label">แปลง</label>
                            <div class="controls">
                                <select id="Unit_Code">
                                    <option>2078-000001</option>
                                    <option>2066-000001</option>
                                    <option>2077-000001</option>
                                    <option>2121-000001</option>
                                </select>
                            </div>
                        </div>

                        <div class="col-xs-12 col-sm-6 col-lg-4">
                            <label class="control-label">บ้านเลขที่</label>
                            <div class="controls">
                                <select id="Unit_Code">
                                    <option>2078-000001</option>
                                    <option>2066-000001</option>
                                    <option>2077-000001</option>
                                    <option>2121-000001</option>
                                </select>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-xs-12 col-sm-6 col-lg-4">
                            <label class="control-label"> วันที่แจ้ง </label>
                            <div class="controls">
                                <input type="date" id="AppointmentDateTo" value="">
                            </div>
                        </div>
                        <div class="col-xs-12 col-sm-6 col-lg-4">
                            <label class="control-label"> ถึง </label>
                            <div class="controls">
                                <input type="date" id="AppointmentDatFrom" value="">
                            </div>
                        </div>

                        <div class="col-xs-12 col-sm-6 col-lg-4">
                            <label class="control-label"></label>
                            <div class="controls">
                                <button type="submit" class="btn btn-primary">Search</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!-- /widget-content -->
    </div>
    <!-- /widget -->
}

